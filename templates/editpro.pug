  <body class="bg-dark" style = "color:white">
    
<div class="container">
  <div class="py-5 text-center">
    <h1>Edit Profile!</h1>
  </div>
  <main style="margin-left: 10%;">
    

    <div class="row g-5">


<form method="POST">
      {% csrf_token %}

       
      <div class="col-md-7 col-lg-8">
        <h4 class="mb-3">Add your Details</h4>
        <form class="needs-validation" novalidate>
          <div class="row g-3">

            <div class="col-12">
              <label for="firstName" class="form-label">First name</label><br>
              <input class="form-control" placeholder="{{name}}" disabled>
              
            </div>

            

            <div class="col-12">
              <label for="email" class="form-label">Email</label><br>
            
              <input class="form-control"  placeholder="{{email}}"disabled>
              
            </div>

            <div class="col-12">
              <label for="address" class="form-label">Phone no.</label>
              <input type="text" class="form-control"  name="phone" placeholder="ex:1234567890" required>
              <div class="invalid-feedback">
                Please enter your Number.
              </div>
            </div>

            <div class="col-12">
              <label for="address" class="form-label">Address</label>
              <input type="text" class="form-control"  name="address" placeholder="1234 Main St" required>
              <div class="invalid-feedback">
                Please enter your address.
              </div>
            </div>

            

            <div class="col-md-5">
              <label for="country" class="form-label">Country</label>
              <select class="form-select" name="country" required>
                <option value="">Choose...</option>
                <option>United States</option>
                <option>India</option>
                <option>England</option>
                <option>Australia</option>
                <option>France</option>
              </select>
              <div class="invalid-feedback">
                Please select a valid country.
              </div>
            </div>

            
            </div>
          </div>
          <hr class="my-4">
  <a href="/">
  <button class="w-100 btn btn-primary btn-lg" type="submit">Continue to Surf!!</button>
</a>
</form>

          

          

       
  </main>
  
</div>
